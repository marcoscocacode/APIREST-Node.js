  "bugs": {
    "url": "https://github.com/marcoscocacode/APIREST-Node.js/issues"
  },
  "homepage": "https://github.com/marcoscocacode/APIREST-Node.js#readme",
  "devDependencies": {
    "nodemon": "^2.0.19"
  },
  "dependencies": {
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "knex": "^2.3.0",
    "mysql2": "^2.3.3"
  }
}
